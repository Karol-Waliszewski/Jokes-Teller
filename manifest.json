{
  "name": "Jokes Teller",
  "version": "1.0",
  "description": "Wanna hear a joke?",
  "manifest_version": 2,
  "permissions": ["declarativeContent","activeTab"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon128.png"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "64": "icon64.png",
    "128": "icon128.png"
  }
}
